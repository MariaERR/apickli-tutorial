{
	"name": "myapi-test",
	"version": "1.0.0",
	"description": "Integration testing for myapi v1",
	"dependencies": {
		"ajv": "8.12.0",
		"apickli": "latest",
		"bytesize": "^1.1.0",
		"content-length": "^1.0.1",
		"cucumber": "^6.0.7",
		"cucumber-html-report": "latest",
		"custom-cucumber-report-generator": "latest",
		"dotenv": "^16.0.3",
		"response-time": "2.3.2"
	},
	"devDependencies": {
		"cucumber" : "latest",
		"@cucumber/cucumber": "latest",
		"cucumber-html-reporter": "latest"
	},
	"scripts": {

		"test": "cucumber-js",
		"test:report": "npm test ./test/features/**/*.feature -f json:./reports/cucumber_report.json && cucumber-html-reporter -o ./reports/cucumber_report.html -j ./reports/cucumber_report.json",
		"cucumber-html-reporter": "node cucumber-html-reporter.js"
	}
}
